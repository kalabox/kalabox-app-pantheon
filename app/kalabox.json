{
  "appName": "panth",
  "appType": "kalabox-app-pantheon",
  "appPlugins": [
    "kalabox-plugin-git",
    "kalabox-plugin-rsync",
    "kalabox-plugin-pantheon"
  ],
  "shareIgnores": [
    "sites/default/files",
    "files",
    "wp-content/uploads",
    "!*modules*"
  ],
  "pluginConf": {
    "kalabox-plugin-pantheon": {
      "account": "",
      "drush-version": "8",
      "env": "",
      "framework": "",
      "php": "",
      "site": "",
      "upstream": ""
    }
  },
  "appComponents": {
    "appserver": {
      "image": {
        "name": "pantheon-appserver",
        "srcRoot": ":appRoot:"
      }
    },
    "db": {
      "image": {
        "name": "pantheon-mariadb",
        "srcRoot": ":appRoot:"
      }
    },
    "solr": {
      "image": {
        "name": "pantheon-solr",
        "srcRoot": ":appRoot:"
      }
    },
    "redis": {
      "image": {
        "name": "pantheon-redis",
        "srcRoot": ":appRoot:"
      }
    },
    "edge": {
      "image": {
        "name": "pantheon-edge",
        "srcRoot": ":appRoot:"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        },
        {
          "port": "443/tcp",
          "default": true,
          "secure": true
        }
      ]
    }
  }
}
